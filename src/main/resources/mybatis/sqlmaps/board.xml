<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ccgm">
    <select id="getBookList" resultType="kr.or.ccgm.model.Board">
        SELECT
        id,
        name,
        writer,
        price,
        genre,
        publisher,
        cnt
        FROM
        ccgm.BOOKS
    </select>
</mapper>